{% for venue in venues %}
	<div class="venue">
		<a href="{{ venue.data.link }}" target="_blank">
		{% image venue.url.slice(1,-1) + "/" + venue.data.image, venue.data.title, "", [512], "100vw" %}
		</a>

		<h3>
			<a href="{{ venue.data.link }}" target="_blank">{{ venue.data.title }}
				<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
					<path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
					<path d="M12 6h-6a2 2 0 0 0 -2 2v10a2 2 0 0 0 2 2h10a2 2 0 0 0 2 -2v-6"></path>
					<path d="M11 13l9 -9"></path>
					<path d="M15 4h5v5"></path>
				</svg>
			</a>
		</h3>
	</div>
{% endfor %}
